{

	"name": "satisfeet-manager",

  "engine": {
    "views": {
      "path": "/usr/templates"
    },
    "builder": {
      "entry": "/app/index.js",
      "aliases": {
        "templates": "../usr/templates"
      },
      "path": "/srv/javascripts/build.js",
      "extensions": [
        ".js",
        ".json",
        ".jade"
      ]
    }
  },

  "static": {
    "path": "/srv"
  },

  "database": {
    "url": "mysql://root@localhost/satisfeet"
  }

}
